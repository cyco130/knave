{
	"name": "knave-react",
	"version": "1.0.0",
	"description": "Complete client-side navigation solution for React",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "rm -rf dist && tsup src/index.tsx --dts --format=cjs,esm --minify",
		"dev": "tsup src/index.tsx --dts --format=esm --watch . --watch ../knave/dist",
		"prepack": "pnpm build"
	},
	"keywords": [
		"react",
		"client-side navigation",
		"history api",
		"scroll restoration",
		"navigation blocking",
		"pushState",
		"replaceState"
	],
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"license": "MIT",
	"peerDependencies": {
		"react": "^16.8 | ^17 | ^18"
	},
	"dependencies": {
		"@types/react": "^17.0.37",
		"knave": "workspace:*"
	},
	"devDependencies": {
		"react": "^17.0.2",
		"tsup": "^5.10.3",
		"typescript": "^4.5.2"
	}
}
