{
	"name": "knave-react",
	"version": "2.0.0-next.0",
	"description": "Complete client-side navigation solution for React",
	"repository": "https://github.com/cyco130/knave",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "rm -rf dist && tsup src/index.tsx --dts-resolve --format=cjs,esm",
		"dev": "tsup src/index.tsx --dts-resolve --format=esm --watch . --watch ../knave/dist",
		"prepack": "pnpm build"
	},
	"keywords": [
		"react",
		"client-side navigation",
		"history api",
		"scroll restoration",
		"navigation blocking",
		"pushState",
		"replaceState"
	],
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"license": "MIT",
	"peerDependencies": {
		"react": "^16.8 | ^17 | ^18"
	},
	"dependencies": {
		"@types/react": "^18.0.21"
	},
	"devDependencies": {
		"knave": "workspace:*",
		"react": "^18.2.0",
		"tsup": "^6.2.3",
		"typescript": "^4.8.3"
	}
}
