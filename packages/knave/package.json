{
	"name": "knave",
	"version": "1.1.1",
	"description": "Framework-agnostic client-side navigation library",
	"repository": "https://github.com/cyco130/knave",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "rm -rf dist && tsup src/index.ts --dts --format=cjs,esm",
		"dev": "tsup src/index.ts --dts --format=esm --watch",
		"prepack": "pnpm build"
	},
	"keywords": [
		"client-side navigation",
		"history api",
		"scroll restoration",
		"navigation blocking",
		"pushState",
		"replaceState"
	],
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"tsup": "^6.2.3",
		"typescript": "^4.8.3"
	}
}
